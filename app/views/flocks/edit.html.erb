<%= render 'layouts/sidebar' %>
<div class="content__details">
  <h1>Editing flock</h1>
  <%= turbo_frame_tag "flock_#{@flock.id}" do %>
    <%= render "form", flock: @flock %>
  <% end %>
  <br>
  <div class="btn">
    <%= link_to "Show this flock", @flock, class: "btn-success" %> 
    <%= link_to "Back to flocks", flocks_path, class: "btn-back" %>
  </div>
</div>
