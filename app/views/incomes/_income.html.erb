<div id="<%= dom_id income %>">
  <p>
    <strong>Date:</strong>
    <%= income.date %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= income.category %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= income.quantity %>
  </p>

  <p>
    <strong>Unit price:</strong>
    <%= income.unit_price %>
  </p>

  <p>
    <strong>Total amount:</strong>
    <%= income.total_amount %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= income.description %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= income.type_id %>
  </p>

</div>
