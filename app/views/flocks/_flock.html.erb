<div id="<%= dom_id flock %>">
  <p>
    <strong>Batch no:</strong>
    <%= flock.batch_no %>
  </p>

  <p>
    <strong>Date in:</strong>
    <%= flock.date_in %>
  </p>

  <p>
    <strong>Retirement date:</strong>
    <%= flock.retirement_date %>
  </p>

  <p>
    <strong>Source:</strong>
    <%= flock.source %>
  </p>

  <p>
    <strong>Flock stored:</strong>
    <%= flock.flock_stored %>
  </p>

  <p>
    <strong>Initial stock:</strong>
    <%= flock.initial_stock %>
  </p>

  <p>
    <strong>Current stock:</strong>
    <%= flock.current_stock %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= flock.age %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= flock.notes %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= flock.status.name %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= flock.type.name %>
  </p>

</div>
